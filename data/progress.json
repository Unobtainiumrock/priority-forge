    {
      "id": "2b4c2da0-d284-45c5-8664-4f795021e56f",
      "date": "2026-01-09",
      "decision": "REST polling for prod deployment (Kafka deferred)",
      "rationale": "Kafka + Flink in PRD is aspirational high-scale architecture for observability layer; REST polling via Go API sufficient for initial prod deployment. Kafka becomes relevant when needing stream processing at scale, event replay/audit capabilities, or decoupling for multiple downstream consumers.",
      "createdAt": "2026-01-10T05:36:30.369Z"
    },
    {
      "id": "9cae5776-173b-49ff-816b-315445776920",
      "date": "2026-01-10",
      "decision": "Adopt ESMM over separate CTR/CVR models",
      "rationale": "Eliminates Sample Selection Bias by training CVR on entire impression space via pCTCVR loss trick",
      "createdAt": "2026-01-10T19:50:56.142Z"
    },
    {
      "id": "329f7b70-42da-41ee-86da-4f062293e9a8",
      "date": "2026-01-10",
      "decision": "Conservative CVR placeholder: Gaussian(mean=1%, std=0.3%, floor=0.1%)",
